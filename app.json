{
  "expo": {
    "name": "texas-web",
    "slug": "texas-web",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "texasweb",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static"
    },
    "plugins": ["expo-router"]
  }
}
